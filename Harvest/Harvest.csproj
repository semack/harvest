<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>netstandard2.0</TargetFramework>
		<Authors>David Bond</Authors>
		<PackageId>Harvest.Core</PackageId>
		<LangVersion>latest</LangVersion>
		<Version>2.4$(VersionSuffix)</Version>
		<Company>Panoramic Data Limited</Company>
		<Description>A wrapper for the Harvest time tracking API, forked from the Github project by Joel Potter, Data Research Group, Paul Irwin, updated to support .NET Standard 2.0.</Description>
		<Copyright>Panoramic Data Limited 2021</Copyright>
		<PackageLicenseFile>LICENSE</PackageLicenseFile>
		<PackageProjectUrl>https://github.com/semack/harvest</PackageProjectUrl>
		<PackageIcon>harvest-logo-icon.png</PackageIcon>
		<RepositoryUrl>https://github.com/semack/harvest.git</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
		<PackageTags>harvest;harvestapp;harvestapi;api;harvesthq;timetracking</PackageTags>
		<PackageReleaseNotes></PackageReleaseNotes>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
        <AssemblyVersion>2.4$(VersionSuffix)</AssemblyVersion>
        <FileVersion>2.4$(VersionSuffix)</FileVersion>
		<AssemblyName>Harvest.Core</AssemblyName>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Newtonsoft.Json" Version="12.0.2" />
		<PackageReference Include="Refit" Version="4.7.51" />
	</ItemGroup>


	<ItemGroup>
		<None Include="LICENSE" Pack="true" PackagePath="" />
		<None Include="harvest-logo-icon.png" Pack="true" PackagePath="" />
	</ItemGroup>

</Project>
